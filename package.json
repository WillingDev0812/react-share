{
  "name": "react-share",
  "version": "3.0.1",
  "description": "Social media share buttons and share counts for React.",
  "main": "./lib/index.js",
  "types": "/lib/index.d.ts",
  "module": "./es/index.js",
  "sideEffects": false,
  "engines": {
    "node": ">=6.9.0",
    "npm": ">=5.0.0"
  },
  "files": [
    "lib/**/*",
    "es/**/*"
  ],
  "scripts": {
    "build": "npm run lint && npm run build:commonjs && npm run build:es",
    "build:commonjs": "rimraf ./lib && tsc -p . --outDir ./lib --module commonjs",
    "build:es": "rimraf ./es && tsc -p . --outDir ./es --module es2015",
    "build-demo": "rimraf ./docs && cross-env NODE_ENV=production webpack -p --config webpack.demo.config.js",
    "run-demos": "webpack-dev-server --hot --config webpack.demo.config.js --progress",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src ./demo",
    "prepublishOnly": "npm run build",
    "start": "npm run run-demos"
  },
  "keywords": [
    "react",
    "component",
    "react-component",
    "social",
    "media",
    "social-media",
    "share",
    "button",
    "count"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nygardk/react-share"
  },
  "author": {
    "name": "Klaus Nyg√•rd",
    "email": "klaus.nygard@gmail.com",
    "url": "http://klausnygard.fi"
  },
  "contributors": [
    {
      "name": "Ari Autio",
      "email": "ari.autio@iki.fi"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/assert": "^1.4.2",
    "@types/classnames": "^2.2.8",
    "@types/prop-types": "^15.7.1",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "7.13.0",
    "file-loader": "4.0.0",
    "html-webpack-plugin": "3.2.0",
    "prettier": "^1.16.4",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-hot-loader": "4.9.0",
    "rimraf": "^2.6.3",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.3.0",
    "ts-loader": "^6.0.2",
    "typescript": "3.5.1",
    "url-loader": "2.0.0",
    "webpack": "4.33.0",
    "webpack-cli": "3.3.3",
    "webpack-dev-server": "3.7.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "jsonp": "^0.2.1",
    "prop-types": "^15.5.8"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  }
}
