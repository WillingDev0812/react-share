{
  "name": "react-share",
  "version": "4.0.0",
  "description": "Social media share buttons and share counts for React.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./es/index.js",
  "sideEffects": false,
  "engines": {
    "node": ">=6.9.0",
    "npm": ">=5.0.0"
  },
  "files": [
    "lib/**/*",
    "es/**/*"
  ],
  "scripts": {
    "build": "npm run lint && npm run build:commonjs && npm run build:es",
    "build:commonjs": "rimraf ./lib && tsc -p . --outDir ./lib --module commonjs",
    "build:es": "rimraf ./es && tsc -p . --outDir ./es --module es2015 --moduleResolution Node",
    "build-demo": "rimraf ./docs && cross-env NODE_ENV=production webpack -p --config webpack.demo.config.js",
    "run-demos": "webpack-dev-server --hot --config webpack.demo.config.js --progress",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src ./demo",
    "prepublishOnly": "npm run build",
    "start": "npm run run-demos"
  },
  "keywords": [
    "react",
    "component",
    "react-component",
    "social",
    "media",
    "social-media",
    "share",
    "button",
    "count"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nygardk/react-share"
  },
  "author": {
    "name": "Klaus Nyg√•rd",
    "email": "klaus.nygard@gmail.com",
    "url": "http://klausnygard.fi"
  },
  "contributors": [
    {
      "name": "Ari Autio",
      "email": "ari.autio@iki.fi"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/assert": "^1.4.3",
    "@types/classnames": "^2.2.9",
    "@types/jsonp": "^0.2.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "cross-env": "6.0.3",
    "css-loader": "3.4.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "file-loader": "5.0.2",
    "html-webpack-plugin": "3.2.0",
    "prettier": "^1.19.1",
    "react": "16.3.0",
    "react-dom": "16.3.0",
    "react-hot-loader": "4.12.18",
    "rimraf": "^3.0.0",
    "style-loader": "1.1.2",
    "terser-webpack-plugin": "2.3.1",
    "ts-loader": "^6.2.1",
    "typescript": "3.7.4",
    "url-loader": "3.0.0",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "jsonp": "^0.2.1"
  },
  "peerDependencies": {
    "react": "^16.3.0"
  }
}
